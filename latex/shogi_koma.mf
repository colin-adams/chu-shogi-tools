  draw z.a..z.b;
  if x.b = x.a:
    tmpx := x.b;
  else:
    tmpx := x.b -  serif_length;
  fi
  if y.b - y.a = 0:
    tmpy := y.b;
  elseif x.b - x.a = 0:
    tmpy := y.b - serif_length;
  else:
    % showvariable tmpx,tmpy;
