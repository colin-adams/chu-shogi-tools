  fi
  if (x.a < x.b) and (x.a > tmpx):
    tmpx := x.a;
  fi
  filldraw z.b--(x.b-.2*serif_length,y.b+.3*serif_height)--(tmpx,tmpy)--z.b..cycle;
enddef;

def serifcurve(suffix a,b,c) =
  pickup pencircle xscaled med yscaled thin;
  draw z.a..z.b..z.c;
  if y.b < y.c:
    serifline(b,c);
  fi
enddef;

def medline(suffix a,b) =
  pickup pencircle xscaled med yscaled thin;
  %draw z.a..z.b;
  draw z.a..(x.b,y.b-.02h);
  pickup pencircle xscaled medserif yscaled thin rotated -20;
  draw z.a;
enddef;

def thinline(suffix a,b) =
  pickup pencircle xscaled thin yscaled hair;
  draw z.a..z.b;
enddef;


def medcurve(suffix a,b,c)(expr d) =
  pickup pencircle xscaled med yscaled thin rotated d;
  draw z.a..z.b..z.c;
enddef;

def righthook(suffix a,b,c,free)(expr d)=
  pickup pencircle xscaled med yscaled thin rotated d;
  draw z.a{down}..tension1.2..z.b..z.c;
  penpos.c(med,d);
  penpos.free(med,87);
  x.free=x.c.r-.025w;
  y.free=y.c+hook_height;
  pickup pencircle scaled 0;
  filldraw z.c.l--z.c.r--z.free.r--z.c.l..cycle;
enddef;

def tensedrighthook(suffix a,b,c,free)(expr d)=
  pickup pencircle xscaled med yscaled thin rotated d;
  draw z.a{down}..tension3..z.b..tension3..z.c;
  penpos.c(med,d);
  penpos.free(med,87);
  x.free=x.c.r-.025w;
  y.free=y.c+hook_height;
  pickup pencircle scaled 0;
  filldraw z.c.l--z.c.r--z.free.r--z.c.l..cycle;
enddef;

def tensedthinrighthook(suffix a,b,c,free)(expr d)=
  pickup pencircle xscaled 1.3thin yscaled 1.5hair rotated d;
  draw z.a{down}..tension3..z.b..tension3..z.c;
  penpos.c(.7med,d);
  penpos.free(.7med,87);
  x.free=x.c.r-.025w;
  y.free=y.c+.7hook_height;
  pickup pencircle scaled 0;
  filldraw z.c.l--z.c.r--z.free.r--z.c.l..cycle;
enddef;

def tensedsmallrighthook(suffix a,b,c,free)(expr d)=
  pickup pencircle xscaled med yscaled thin rotated d;
  draw z.a{down}..tension3..z.b..tension3..z.c;
  penpos.c(thin,d);
  penpos.free(thin,87);
  x.free=x.c.r-.025w;
  y.free=y.c+.3hook_height;
  pickup pencircle scaled 0;
  filldraw z.c.l--z.c.r--z.free.r--z.c.l..cycle;
enddef;

def thinthickcurve(suffix a,b,c)(expr d) =
  penpos.a(hair,d);
  penpos.b(.75med,d);
  penpos.c(med,d);
  penstroke z.a.e..z.b.e..z.c.e;
enddef;

def hairthinmedcurve(suffix a,b,c)(expr d) =
  penpos.a(hair,d);
  penpos.b(thin,d);
  penpos.c(med,d);
  penstroke z.a.e..z.b.e..z.c.e;
enddef;

def hairthinthincurve(suffix a,b,c)(expr d) =
  penpos.a(hair,d);
  penpos.b(thin,d);
  penpos.c(1.2thin,d);
  penstroke z.a.e..tension2..z.b.e..tension2..z.c.e;
enddef;

def hairmedmedtensedcurve(suffix a,b,c)(expr t,d) =
  penpos.a(hair,d);
  penpos.b(med,d);
  penpos.c(med,d);
  penstroke z.a.e..z.b.e..tension.t..z.c.e;
enddef;

def hairthinmedtensedcurve(suffix a,b,c)(expr t,d) =
  penpos.a(hair,d);
  penpos.b(1.2thin,d);
  penpos.c(.9med,d);
  penstroke z.a.e..z.b.e..tension.t..z.c.e;
enddef;

def thickthincurve(suffix a,b,c)(expr d) =
  penpos.a(med,d);
  penpos.b(med,d);
  penpos.c(hair,d);
  penstroke z.a.e..z.b.e..z.c.e;
  pickup pencircle xscaled medserif yscaled thin rotated d;
  draw z.a;
enddef;

def thinthickline(suffix a,b,c)(expr d) =
    penpos.a(thin,d);
    penpos.b(med,d);
    x.c := (x.a+x.b)/2;
    y.c := (y.a+y.b)/2-.2medserif; % should be proportional to length
    penpos.c(0.8 * med,d);
    pickup pencircle xscaled medserif yscaled thin rotated d;
    draw z.b;
    penstroke z.a.e..z.c.e..z.b.e;
enddef;  

def hairthickline(suffix a,b,c)(expr d) =
    penpos.a(hair,d);
    penpos.b(med,d);
    x.c := (x.a+x.b)/2;
    y.c := (y.a+y.b)/2-.4medserif; % should be proportional to length
    penpos.c(0.6 * med,d);
    pickup pencircle xscaled medserif yscaled thin rotated d;
    draw z.b;
    penstroke z.a.e..z.c.e..z.b.e;
enddef;  

def triangle(suffix a,b)(expr d) =
  penpos.a(med,d);
  penpos.b(hair,d);
  penstroke z.a.e..z.b.e;
enddef;

def drop(suffix a,b) =
  penpos.a(hair,20);
  penpos.b(med,20);
  penstroke z.a.e..tension3..{dir-70}z.b.e;
  pickup pencircle xscaled medserif yscaled thin rotated 19;
  draw z.b;
enddef;

def leftsmallhook(suffix a,b) =
  penpos.a(med,-20);
  penpos.b(med, 20);
  penstroke z.a.e..z.b.e;
  pickup pencircle xscaled medserif yscaled thin rotated -20;
  draw z.a;
  pickup pencircle xscaled medserif yscaled thin rotated 20;
  draw z.b;
  pickup pencircle scaled 0;
  filldraw (x.b.r,y.b.r){down}..{left}(x.b.l,y.b.r-yhook)..(x.b.l,y.b.r-yhook){dir-220}..(x.b.l-xhook,y.b.r)..(x.b.l-xhook,y.b.r){right}..(x.b.r,y.b.r)..cycle;
enddef;

def lionhook(suffix a,b,c) =
  penpos.a(hair,0);
  penpos.b(med, 0);
  penpos.c(med, -40);
  penstroke z.a.e{curl2}..z.b.e{down}..z.c.e;
  pickup pencircle xscaled medserif yscaled thin rotated 20;
  %draw z.c;
  pickup pencircle scaled 0;
  penlabels(a,b,c);
  filldraw (x.c.r,y.c.r){dir-130}..(x.c.l,y.c-yhook)..(x.c.l,y.c-yhook){dir-220}..(x.c.l-xhook,y.c.l)..(x.c.l-xhook,y.c.l){right}..(x.c.r,y.c.l)..(x.c.r,y.c.l)..cycle;
  % filldraw z.a.l..z.b.l..z.c.l
  %filldraw(x.c.r,y.c.r)..(x.c.l,y.c-yhook)..(x.c.l,y.c-yhook){dir-220}..(x.c.l-xhook,y.c)..(x.c.l-xhook,y.c)..{up}(x.c.r,y.c.r)..cycle;
enddef;

