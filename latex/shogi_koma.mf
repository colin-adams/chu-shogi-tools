define_pixels(em,cap,serif_height,serif_length);
define_blacker_pixels(thin,med,thick,hair);
define_blacker_pixels(thickserif,medserif,xhook,yhook,hook_height);

def turned(expr p) =
   addto currentpicture also p rotated 180 shifted (w,h);
enddef;
   
def serifline(suffix a, b) =
  save tmpx, tmpy;
  pickup pencircle xscaled med yscaled thin;
  draw z.a..z.b;
  if x.b = x.a:
    tmpx := x.b;
  else:
    tmpx := x.b - serif_length;
  fi
  if y.b - y.a = 0:
    tmpy := y.b;
  elseif x.b - x.a = 0:
    tmpy := y.b - serif_length;
  else:
    % showvariable tmpx,tmpy;
    tmpy := y.b - (y.b-y.a) / (x.b-x.a) * tmpx;
  fi
  if (y.a < y.b) and (y.a > tmpy):
    tmpy := y.a;
  fi
  if (x.a < x.b) and (x.a > tmpx):
    tmpx := x.a;
  fi
  filldraw z.b--(x.b-.35*serif_length,y.b+.5serif_height)--(tmpx,tmpy)--z.b..cycle;
enddef;

def smallserifline(suffix a, b) =
  save tmpx, tmpy;
  pickup pencircle xscaled med yscaled thin;
  draw z.a..z.b;
  if x.b = x.a:
    tmpx := x.b;
  else:
    tmpx := x.b - .5serif_length;
  fi
  if y.b - y.a = 0:
    tmpy := y.b;
  elseif x.b - x.a = 0:
    tmpy := y.b - .35serif_length;
  else:
    % showvariable tmpx,tmpy;
    tmpy := y.b - (y.b-y.a) / (x.b-x.a) * tmpx;
  fi
  if (y.a < y.b) and (y.a > tmpy):
    tmpy := y.a;
  fi
  if (x.a < x.b) and (x.a > tmpx):
    tmpx := x.a;
  fi
  filldraw z.b--(x.b-.15*serif_length,y.b+.2serif_height)--(tmpx,tmpy)--z.b..cycle;
enddef;

def hairserifline(suffix a, b) =
  save tmpx, tmpy;
  pickup pencircle scaled hair;
  draw z.a..z.b;
  if x.b = x.a:
    tmpx := x.b;
  else:
    tmpx := x.b -  serif_length;
  fi
  if y.b - y.a = 0:
    tmpy := y.b;
  elseif x.b - x.a = 0:
    tmpy := y.b - serif_length;
  else:
    % showvariable tmpx,tmpy;
    tmpy := y.b - (y.b-y.a) / (x.b-x.a) * tmpx;
  fi
  if (y.a < y.b) and (y.a > tmpy):
    tmpy := y.a;
  fi
  if (x.a < x.b) and (x.a > tmpx):
    tmpx := x.a;
  fi
  filldraw z.b--(x.b-.2*serif_length,y.b+.3*serif_height)--(tmpx,tmpy)--z.b..cycle;
enddef;

